<Window x:Class="BD3Trab4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BD3Trab4"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Margin="4" Click="Button_Click" >Configurar Connection string</Button>
            <Button Margin="4" Click="ButtonBase_OnClick">Cadastrar Competidores</Button>
            <Button Margin="4" Name="btnCriarSerie" Click="btnCriarSerie_Click">Criar Series</Button>
            <Button Margin="4" Name="btnCadastrarSeries" Click="btnCadastrarSeries_Click">Cadastrar Series</Button>
            <Button Margin="4" Name="btnTerminarSeries" Click="BtnTerminarSeries_OnClick">Terminar Series</Button>
        </StackPanel>
    </Grid>
</Window>