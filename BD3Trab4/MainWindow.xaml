<Window x:Class="BD3Trab4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BD3Trab4"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Center">
            <Button Click="ButtonBase_OnClick">Cadastrar Competidores</Button>
            <Button Click="FecharInscriçõesCompetidores_Click" >Finalizar inscrições de competidores</Button>
            <Button Click="ReabrirInscriçõesCompetidores_Click"  >Reabrir inscrições de competidores</Button>
        </StackPanel>
    </Grid>
</Window>
